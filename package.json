{
    "name": "ml-playground",
    "type": "module",
    "module": "src/index.ts",
    "scripts": {
        "bench": "bun run benchmarks/run_bun.ts",
        "bench:bun": "bun run benchmarks/run_bun.ts",
        "bench:iris": "bun run download:iris && bun run benchmarks/run_bun.ts",
        "bench:clean": "bash -lc '> benchmarks/results.jsonl'",
        "download:iris": "bun run scripts/download_iris.ts",
        "test": "bun test",
        "test:watch": "bun test --watch",
        "typecheck": "bun x tsc -p tsconfig.json --noEmit"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/node": "^24.6.2"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    }
}